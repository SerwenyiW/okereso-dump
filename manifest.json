{
  "manifest_version": 3,
  "name": "okereso dumper",
  "version": "1.0.0",
  "description": "Szerezd meg az összes embert okeresőről!",
  "permissions": [
    "activeTab",
    "scripting",
    "cookies",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["https://okereso.hu/*"],
      "js": ["main.js"]
    }
  ]
}